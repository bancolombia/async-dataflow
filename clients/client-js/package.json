{
  "name": "@bancolombia/chanjs-client",
  "version": "0.2.1",
  "description": "AsycnDataflow websocket browser client. By Bancolombia",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "nyc mocha --reporter spec --reporter mocha-sonarqube-reporter --reporter-options output=coverage/xunit.xml --require source-map-support/register --require jsdom-global/register --require ts-node/register 'test/**/*.test.ts'",
    "test-local": "mocha --require jsdom-global/register --require source-map-support/register --require ts-node/register 'test/**/*.test.ts'",
    "watch:build": "tsc --watch",
    "sonar-scanner": "node_modules/sonar-scanner/bin/sonar-scanner",
    "prepare": "npm run build",
    "build": "tsc",
    "publish-local": "npm run build && npm unpublish --registry http://localhost:4873 && npm publish --registry http://localhost:4873 && echo 'Deployed!'",
    "lint": "eslint --ext .ts ."
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "coverage",
      "dist",
      "src/index.ts",
      "**/*.d.ts",
      "**/*.test.ts"
    ],
    "reporter": [
      "lcov"
    ],
    "all": true
  },
  "keywords": [
    "async",
    "notifications",
    "websocket"
  ],
  "author": "Daniel Bustamante Ospina <danibust@bancolombia.com.co>",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.1.6",
    "@types/mocha": "^5.2.5",
    "@types/pusher-js": "^4.2.0",
    "@types/sinon": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "chai": "^4.2.0",
    "deps": "^1.0.0",
    "eslint": "^8.49.0",
    "fast-text-encoding": "^1.0.3",
    "fetch-mock": "^12.3.0",
    "jsdom": "22.1.0",
    "jsdom-global": "3.0.2",
    "mocha": "^11.1.0",
    "mocha-sonarqube-reporter": "^1.0.2",
    "mock-socket": "^9.0.6",
    "node-fetch": "^2.6.1",
    "nyc": "^15.1.0",
    "sinon": "^6.3.5",
    "sonar-scanner": "^3.1.0",
    "source-map-support": "^0.5.9",
    "ts-mock-imports": "^1.2.1",
    "ts-mockito": "^2.3.1",
    "ts-node": "^7.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "lru-cache": "^10.0.1"
  }
}
